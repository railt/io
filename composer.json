{
    "name": "railt/io",
    "license": "MIT",
    "type": "library",
    "homepage": "http://railt.org",
    "description": "The Railt filesystem utils",
    "keywords": [
        "file",
        "filesystem",
        "fso",
        "input",
        "output",
        "utils"
    ],
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "support": {
        "issues": "https://github.com/railt/railt/issues",
        "source": "https://github.com/railt/io"
    },
    "require": {
        "php": ">=7.1.3",
        "ext-spl": "*",
        "railt/exception": "1.4.*|1.4.x-dev",
        "railt/position": "1.4.*|1.4.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Railt\\Component\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "autoload-dev": {
        "psr-4": {
            "Railt\\Tests\\Io\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "1.3.x": "1.3.x-dev",
            "1.4.x": "1.4.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
