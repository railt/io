{
    "name": "railt/io",
    "type": "library",
    "description": "The Railt filesystem utils",
    "version": "1.3.0",
    "license": "MIT",
    "keywords": [
        "file",
        "filesystem",
        "fso",
        "input",
        "output",
        "utils"
    ],
    "homepage": "http://railt.org",
    "support": {
        "issues": "https://github.com/railt/railt/issues",
        "source": "https://github.com/railt/io"
    },
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-spl": "*"
    },
    "autoload": {
        "psr-4": {
            "Railt\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "autoload-dev": {
        "psr-4": {
            "Railt\\Tests\\Io\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
